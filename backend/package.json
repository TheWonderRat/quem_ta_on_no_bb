{
  "name": "quem_ta_on_no_bb",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server/server.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build-local": "env $(cat .env) tsc --project tsconfig.json",
    "migrate-local": "env $(cat .env) npx sequelize-cli db:migrate",
    "db:drop-local": "env $(cat .env) npx sequelize-cli db:drop",
    "db:up-local": "env $(cat .env) npx sequelize-cli db:create",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "tsnd --rs --transpile-only ./src/server/server.ts",
    "lint": "eslint ./src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration-run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -d ./src/database/typeorm/index.ts",
    "migration-revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert -d ./src/database/typeorm/index.ts",
    "load-db-data": "ts-node src/database/loadDb.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "connect-db": "tsnd ./src/database/ORM/connection.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.35.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.16",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.10",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "tsc-node": "^0.0.3",
    "tsconfig-paths": "^3.14.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  }
}
