{
  "name": "quem_ta_on_no_bb",
  "version": "1.0.0",
  "description": "",
  "main": "./build/app/server/server.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "db:up": "npx sequelize-cli db:migrate",
    "prestart": "npm run build && npm run db:up",
    "start": "node .",
    "build-local": "env $(cat .env) tsc --project tsconfig.json",
    "migrate-local": "env $(cat .env) npx sequelize-cli db:migrate",
    "db:drop-local": "env $(cat .env) npx sequelize-cli db:drop",
    "db:create-local": "env $(cat .env) npx sequelize-cli db:create",
    "db:up-local": "npm run db:drop-local && npm run db:create-local && npm run migrate-local",
    "predev": "npm run build-local && npm run db:up-local",
    "dev": "env $(cat .env) tsnd --rs ./src/app/server/server.ts",
    "lint": "eslint ./src --ext .ts",
    "test": "jest --verbose --forceExit",
    "test:coverage": "env $(cat .env) jest --coverage --watchAll --verbose --forceExit",
    "test-connection-db": "tsnd ./src/database/ORM/connection.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "express": "^4.17.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
